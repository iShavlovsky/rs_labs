{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowJs": true,
    "importHelpers": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "jsxImportSource": "vue",
    "noImplicitThis": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "noEmit": true,
    "lib": [
      "ESNext",
      "DOM",
      "dom.iterable",
      "scripthost"
    ],
    "typeRoots": [
      "node_modules/@types",
      "src/types",
      "./types"
    ],
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@images/*": [
        "./src/assets/images/*"
      ],
      "@fonts/*": [
        "./src/assets/fonts/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@store/*": [
        "./src/store/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@composables/*": [
        "./src/composables/*"
      ],
      "@injections/*": [
        "./src/injections/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@middlewares/*": [
        "./src/middlewares/*"
      ],
      "@plugins/*": [
        "./src/plugins/*"
      ],
      "@router/*": [
        "./src/router/*"
      ],
      "@utility/*": [
        "./src/utils/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ]
    }
  },
  "types": [
    "swiper/vue"
  ],
  "include": [
    "./typed-router/typed-router.d.ts",
    "src/**/*.ts",
    "src/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "src/types/*",
    "./types/*",
    "src/types",
    "./types"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "exclude": [
    "../node_modules",
    "../node_modules/nuxt/node_modules",
    "../dist",
    "../dist-ssr"
  ],
  "vueCompilerOptions": {
    "jsxTemplates": true,
    "experimentalRfc436": true
  }
}
